{
  "Address": {
    "properties": {
      "id": {
        "type": "long"
      },
      "address": {
        "type": "char",
        "length": 42,
        "unique": true
      },
      "balance": {
        "type": "bignumber"
      }
    }
  },
  "Block": {
    "primaryKeys": ["index"],
    "properties": {
      "index": {
        "type": "int",
        "autoIncrement": false
      },
      "hash": {
        "type": "char",
        "length": 66,
        "unique": true
      },
      "timeMined": {
        "type": "datetime"
      }
    }
  },
  "Currency": {
    "properties": {
      "id": {
        "type": "int"
      },
      "name": {
        "type": "string"
      }
    }
  },
  "LastBlock": {
    "primaryKeys": ["currency"],
    "properties": {
      "currency": {
        "type": "int"
      },
      "blockIndex": {
        "type": "long",
        "nullable": true
      }
    }
  },
  "Transaction": {
    "properties": {
      "id": {
        "type": "long"
      },
      "status": {
        "type": "short"
      },
      "txid": {
        "type": "char",
        "length": 66
      },
      "currency": {
        "type": "Currency"
      },
      "fee": {
        "type": "bignumber"
      },
      "nonce": {
        "type": "long"
      },
      "timeReceived": {
        "type": "datetime"
      },
      "blockIndex": {
        "type": "long"
      }
    }
  },
  "TxIn": {
    "table": {
      "name": "txins"
    },
    "primaryKeys": ["transaction", "index"],
    "properties": {
      "transaction": {
        "type": "Transaction"
      },
      "index": {
        "type": "int"
      },
      "sourceTransaction": {
        "type": "Transaction",
        "nullable": true
      },
      "sourceIndex": {
        "type": "long",
        "nullable": true
      },
      "scriptSigHex": {
        "type": "string",
        "nullable": true
      },
      "scriptSigAsm": {
        "type": "string",
        "nullable": true
      },
      "sequence": {
        "type": "long"
      },
      "address": {
        "type": "Address",
        "nullable": true
      },
      "amount": {
        "type": "bignumber",
        "nullable": true
      },
      "valueSat": {
        "type": "bignumber",
        "nullable": true
      },
      "coinbase": {
        "type": "string",
        "nullable": true
      }
    }
  },
  "TxOut": {
    "table": {
      "name": "txouts"
    },
    "primaryKeys": ["transaction", "index"],
    "properties": {
      "transaction": {
        "type": "Transaction"
      },
      "index": {
        "type": "int"
      },
      "scriptPubKeyHex": {
        "type": "string"
      },
      "scriptPubKeyAsm": {
        "type": "string"
      },
      "address": {
        "type": "Address"
      },
      "amount": {
        "type": "bignumber"
      },
      "spentTxId": {
        "type": "string",
        "nullable": true
      },
      "spentHeight": {
        "type": "long",
        "nullable": true
      },
      "spentIndex": {
        "type": "long",
        "nullable": true
      }
    }
  }
}